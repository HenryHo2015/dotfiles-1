!vim: set fenc=utf-8:nu:ai:si:et:ts=4:sw=4:ft=xdefaults:
!You must re-run xrdb ~/.Xresources every time you change the file

! urxvt ----------------------------------------------------------------------

! general
URxvt.preeditType:Root
URxvt.cursorBlink:True
URxvt.saveLines:10240

! foreground and background
URxvt.foreground: #708284
urxvt*background: #07242E

! disable the scrollbar
URxvt.scrollBar: False

! alerm
URxvt.urgentOnBell: True

! font
URxvt.font:xft:DejaVu Sans Mono:pixelsize=16:antialias=false,\
xft:AR PL UMing TW:pixelsize=18:antialias=false

! URL
URxvt.perl-ext-common:  default,matcher
URxvt.url-launcher:      /usr/bin/firefox
URxvt.matcher.button:   1
URxvt.colorUL: #4682B4

! tango color scheme
URxvt*color0: #1e1e1e
URxvt*color1: #cc0000
URxvt*color2: #4e9a06
URxvt*color3: #c4a000
URxvt*color4: #3465a4
URxvt*color5: #75507b
URxvt*color6: #0b939b
URxvt*color7: #d3d7cf
URxvt*color8: #555753
URxvt*color9: #ef2929
URxvt*color10: #8ae234
URxvt*color11: #fce94f
URxvt*color12: #729fcf
URxvt*color13: #ad7fa8
URxvt*color14: #00f5e9
URxvt*color15: #eeeeec
